{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": 0,
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
    }
}
